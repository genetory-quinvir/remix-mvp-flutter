{
  "hosting": {
    "public": "build/web",
    "headers": [ {
      "source": "**",
      "headers": [ {
        "key": "Cross-Origin-Resource-Policy",
        "value": "cross-origin"
      }, {
        "key": "Access-Control-Allow-Origin",
        "value": "*"
      }, {
        "key": "Cross-Origin-Embedder-Policy",
        "value": "credentialless"
      }, {
        "key": "Cross-Origin-Opener-Policy",
        "value": "same-origin"
      } ]
    } ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}